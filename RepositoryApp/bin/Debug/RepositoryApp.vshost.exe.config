        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="DataDirectory" value="..\..\Resources"/>
    <add key = "EventsTxt" value = "EventsTxt"/>
    <add key = "EventsXml" value = "EventsXml"/>
